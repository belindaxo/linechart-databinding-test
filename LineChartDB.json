{
	"name": "Line Chart (Webpack + DB test)",
	"description": "Highcharts line chart with data binding",
	"eula": "",
	"vendor": "",
	"license": "",
    "supportsLinkedAnalysisFilterOnSelection": true,
	"id": "com.sap.sample.linechartdb",
	"newInstancePrefix": "LineChartDB",
	"version": "0.0.1",
	"webcomponents": [
        {
			"kind": "main",
			"tag": "com-sap-sample-linechartdb",
            "url": "https://belindaxo.github.io/linechart-databinding-test/dist/bundle.js",
            "integrity": "",
            "ignoreIntegrity" : true
        },
        {
            "kind": "styling",
            "tag": "com-sap-sample-linechartdb-aps",
            "url": "https://belindaxo.github.io/linechart-databinding-test/dist/bundle.js",
            "integrity": "",
            "ignoreIntegrity" : true
        }
    ],
	"properties": {
        "width": {
            "type": "integer",
            "default": 600
        },
        "height": {
            "type": "integer",
            "default": 400
        },
        "chartTitle": {
            "type": "string",
            "description": "The title of the chart",
            "default": ""
        },
        "chartSubtitle": {
            "type": "string",
            "description": "The subtitle of the chart",
            "default": ""
        },
        "numberFormat": {
            "type": "string",
            "description": "The scale of the numbers (unformatted, k, m, b)",
            "default": "unformatted"
        },
        "decimalPlaces": {
            "type": "integer",
            "description": "The number of decimal places to display",
            "default": 2
        }
	},
	"methods": {
	},
	"events": {
	},
    "dataBindings": {
        "dataBinding": {
            "feeds": [
                {
                    "id": "dimensions",
                    "description": "Dimensions",
                    "type": "dimension"
                },
                {
                    "id": "measures",
                    "description": "Measures",
                    "type": "mainStructureMember"
                }
            ]
        }
    }
}