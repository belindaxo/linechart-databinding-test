{
	"name": "Line Chart (Webpack + DB test)",
	"description": "Highcharts line chart with data binding",
	"eula": "",
	"vendor": "",
	"license": "",
	"id": "com.sap.sample.linechartdb",
	"newInstancePrefix": "LineChartDB",
	"version": "0.0.1",
	"webcomponents": [
        {
			"kind": "main",
			"tag": "com-sap-sample-linechartdb",
            "url": "https://belindaxo.github.io/linechart-databinding-test/dist/bundle.js",
            "integrity": "",
            "ignoreIntegrity" : true
        },
        {
            "kind": "styling",
            "tag": "com-sap-sample-linechartdb-aps",
            "url": "https://belindaxo.github.io/linechart-databinding-test/dist/bundle.js",
            "integrity": "",
            "ignoreIntegrity" : true
        }
    ],
	"properties": {
        "width": {
            "type": "integer",
            "default": 600
        },
        "height": {
            "type": "integer",
            "default": 400
        },
        "title": {
            "type": "string",
            "default": "Line Chart"
        }
	},
	"methods": {
        "setTitle": {
            "description": "Sets the title of the chart.",
            "parameters": [
                {
                    "name": "newTitle",
                    "type": "string",
                    "description": "The new title of the chart."
                }
            ],
            "body": "this.title = newTitle;"
        },
        "getTitle": {
            "returnType": "string",
            "description": "Returns the title of the chart.",
            "body": "return this.title;"
        }
	},
	"events": {
        "onClick": {
            "description": "Called when the user clicks the widget."
        }
	},
    "dataBindings": {
        "dataBinding": {
            "feeds": [
                {
                    "id": "dimensions",
                    "description": "Dimensions",
                    "type": "dimension"
                },
                {
                    "id": "measures",
                    "description": "Measures",
                    "type": "mainStructureMember"
                }
            ]
        }
    }
}